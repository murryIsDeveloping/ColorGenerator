/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "src",
    "paths": {
      "@modules/*": [
        "modules/*"
      ],
      "@providers/*": [
        "providers/*"
      ],
      "@util/*": [
        "util/*"
      ],
      "@env/*": [
        "environments/*"
      ],
      "@app/*": [
        "modules/app/*"
      ],
      "@shared/*": [
        "modules/shared/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
